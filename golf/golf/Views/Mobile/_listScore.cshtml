@model IEnumerable<golf.Models.PersonGolfer>

<h2>Din placering</h2>

<table id="othercompetetors" class="othercompetetors">
    <thead>
        <tr>
            <td>
               @Html.Label("Spelare")
            </td>

            <td>
                @Html.Label("+/-")
            </td>
            <td>
                @Html.Label("Antal spelade hål")
            </td>
        </tr>
    </thead>
    <tbody>
        @foreach(var i in Model)
        { 
        <tr>
            <td>
               @Html.Label(i.firstName+ " "+i.lastName)
            </td>

            <td>
                @Html.Label(i.toPar.ToString())
            </td>
                <td>
              @Html.Label(i.playedHoles.ToString())
            </td>
        </tr>
        }

    </tbody>
</table>