@model golf.Models.AddCompPlayer

<table>
    @foreach (var item in Model.golfers)
    {


        <tr>
            <th>
                @Html.DisplayFor(o => item.firstName)
                
            </th>
            <th>
                @Html.DisplayFor(o => item.lastName)
            </th>
            <th>
                <div class="buttonGreen">

                    @Html.ActionLink("Lägg till",
                        "RegisterPlayer", new { golferid = item.golfid, competitionid = Model.comp.Id })


                </div>
            </th>
        </tr>
    }
</table>
