@model golf.Models.AddCompPlayer
<div class="addplayer" id="addplayer">
    <h2>@Model.comp.name</h2>
    <h3>@Model.comp.cDate</h3>
    <div>
        <input id="searchButton" type="submit" onclick="soP(1)" value="Sök" />
        <input id="searchField" type="text" placeholder="sök" />

    </div>
    <div id=" sDiv">

        
    </div>

</div>
<script>

    function soP(d) {

        var s = document.getElementById('searchField').value;

        $.ajax({
            url: '/MemberManagement/!!!!',
            contentType: 'application/html; charset=utf-8',
            data: { searchstring: s },
            type: 'GET',
            dataType: 'html'

        })

        .success(function (result) {
            $('#sDiv').html(result);
        })
        .error(function (xhr, status) {
            alert(status);
        })


    }



</script>