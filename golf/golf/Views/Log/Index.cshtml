@model golf.Models.Person


@{
    ViewBag.Title = "Index";
}

@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h2>@ViewBag.Message</h2>
            </hgroup>
            <p>
                @Html.ActionLink("Är du inte medlem? Klicka här", "Egister")
            </p>
        </div>
    </section>
}

@using (Html.BeginForm("LogInCheck", "Log"))
{ 
    @Html.Label("E-post")
   
    <br />
    @Html.Label("Lösenord")
    
    @Html.TextBoxFor(m => m.email)
    @Html.TextBoxFor(m => m.PW)
    

    <input type="submit" value="LogInCheck" />
}
<p>
    
